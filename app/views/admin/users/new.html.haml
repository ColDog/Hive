%div.container-fluid
  %div.col-md-2.sidebar
    = render 'admin/sidebar', active: 'users'
  %div.col-md-10
    %h1 Users
    %ul.nav.nav-tabs
      %li{ role: 'presentation' }
        = link_to 'Users', admin_users_path
      %li.active{ role: 'presentation' }
        = link_to 'New User', new_admin_user_path

    = render 'admin/users/form', supply: @user, url: admin_users_path, method: :post