- cache set_keys([@user, @user.organization_members, Attachment, SupplyList], params) do
  %div.container-fluid
    %div.col-md-2.sidebar.hidden-sm.hidden-xs
      = render 'admin/shared/sidebar', active: 'Users'
    %div.col-md-10.shad-left
      %h1 Users
      %ul.nav.nav-tabs
        %li{ role: 'presentation' }
          = link_to 'Users', admin_users_path
        %li{ role: 'presentation' }
          = link_to 'New User', new_admin_user_path
        %li.active{ role: 'presentation' }
          = link_to @user.name, '#'

      %br
      %div.col-md-4
        - cache set_keys([@user]) do
          = render 'admin/users/form', url: admin_user_path(@user), method: :put, user: @user
      %div.col-md-8
        - cache set_keys([@user, @user.organization_members, Attachment, SupplyList]) do
          = render 'admin/users/list', user: @user
