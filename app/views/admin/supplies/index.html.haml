= render 'admin/shared/index',  data: @supplies,
                                header: %w"Name Total In-Use Notes Actions",
                                methods: [['Link', :name, :edit_admin_supply_path], ['Double', :supply_lists, :count], :in_use, :notes],
                                links: [ ['edit', :edit_admin_supply_path], ['delete', :admin_supply_path, method: :delete, data: { confirm: 'This will delete the record in the database permanently.' }] ],
                                new: ['New Supply', :new_admin_supply_path],
                                main: ['Supplies', :admin_supplies_path],
                                title: 'Supplies',
                                search_location: false


-# data = variable
-# header = array table headers
-# methods = array of table methods
-# links = array of table links
-# new = array with ['link', new_something_path]
-# main = array with ['link', somethings_path]
-# title = title