<script>
	<% cache(Organization.cache_key(params)) do %>
		var orgs = <%=raw Organization.select_values.to_json %>;
	<% end %>
	<% cache(User.cache_key(params)) do %>
		var users = <%=raw User.select_values.to_json %>;
	<% end %>
	$( document ).ready(function() {
		$('.org-sel').each(function(i, obj) {
			$.each(orgs, function (i, item) {
				$(obj).append($('<option>', {
					value: item.value,
					text : item.text
				}));
			});
		});
		$('.usr-sel').each(function(i, obj) {
			$.each(users, function (i, item) {
				$(obj).append($('<option>', {
					value: item.value,
					text : item.text
				}));
			});
		});
	});
</script>