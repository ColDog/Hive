= render 'admin/shared/new',  title: 'Organizations',
                              main: ['Organizations', :admin_organizations_path],
                              new: ['New Organization', :new_admin_organization_path],
                              form_location: 'admin/organizations/form',
                              form_variable: @organization,
                              form_url: :admin_organizations_path,
                              form_method: :post

-# title = title
-# main = [mainlink, main_link_path]
-# new = ['new', new_path]
-# form_location = render path
-# form_variable = form variable
-# form_url = form_path
-# form_method = ie: :post