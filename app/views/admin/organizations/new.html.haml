- cache do
  %div.container-fluid
    %div.col-md-2.sidebar.hidden-sm.hidden-xs
      = render 'admin/shared/sidebar', active: 'Organizations'
    %div.col-md-10.shad-left
      %h1 Organizations
      %ul.nav.nav-tabs
        %li{ role: 'presentation' }
          = link_to 'Organizations', admin_organizations_path
        %li.active{ role: 'presentation' }
          = link_to 'New Organization', new_admin_organization_path

      %br
      %div.col-md-4
        - cache @organization.cache_key(params) do
          = render 'admin/organizations/form', url: admin_organizations_path, method: :post, org: @organization
