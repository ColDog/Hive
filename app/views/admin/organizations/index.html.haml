= render 'admin/shared/index',  data: @organizations,
                                header: %w"Name Employees Description Tags Actions",
                                methods: [:name, [:users,:count], :description, :tags],
                                links: [ ['edit', :edit_admin_organization_path], ['delete', :admin_organization_path, method: :delete] ],
                                new: ['New Organization', :new_admin_organization_path],
                                main: ['Organizations', :admin_organizations_path],
                                title: 'Organizations'


-# data = variable
-# header = array table headers
-# methods = array of table methods
-# links = array of table links
-# new = array with ['link', new_something_path]
-# main = array with ['link', somethings_path]
-# title = title