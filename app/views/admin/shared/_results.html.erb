<div class="modal fade" id="results" tabindex="-1" role="dialog" aria-labelledby="results">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				<h4 class="modal-title" id="myModalLabel">Results</h4>
			</div>
			<div class="modal-body" style="max-height: 500px;overflow: scroll;">
				<div id="result">Results from the latest upload. Wait a few minutes, as it may take a while to write to the database or try uploading again if you still don't see anything.</div>
			</div>
		</div>
	</div>
</div>



<script>
	$("form").bind("ajax:remotipartSubmit", function(e, data, settings){
		settings.dataType = "html *";
		console.log('submit')
	});
	$("form").bind("ajax:success", function(xhr, data, status){
		console.log('success');
		$.get("/admin/imports/results/<%=raw key %>.html", function( data ) {
			$('#result').html(data)
		});
		$('#results').modal('show');
	});
</script>